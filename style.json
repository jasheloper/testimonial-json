  "content": "<!-- wp:columns {\"style\":{\"color\":{\"gradient\":\"linear-gradient(0deg,rgb(25,40,30) 9%,rgb(110,140,100) 56%)\"},\"spacing\":{\"padding\":{\"top\":\"0px\",\"bottom\":\"0px\",\"left\":\"120px\"},\"margin\":{\"top\":\"0\",\"bottom\":\"0\"}}}} -->\n<div class=\"wp-block-columns has-background\" style=\"background:linear-gradient(0deg,rgb(25,40,30) 9%,rgb(110,140,100) 56%);margin-top:0;margin-bottom:0;padding-top:0px;padding-bottom:0px;padding-left:120px\"><!-- wp:column {\"verticalAlignment\":\"top\"} -->\n<div class=\"wp-block-column is-vertically-aligned-top\"><!-- wp:image {\"id\":10727,\"sizeSlug\":\"large\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-large\"><img src=\"https://predragwp.store/wp-content/uploads/2023/06/HQ6-2624x3936-1-scaled-e1707457689291-1024x771.jpg\" alt=\"\" class=\"wp-image-10727\" /></figure>\n<!-- /wp:image --></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"style\":{\"spacing\":{\"padding\":{\"top\":\"var:preset|spacing|50\",\"bottom\":\"var:preset|spacing|50\",\"left\":\"0\",\"right\":\"0\"},\"blockGap\":\"0\"}}} -->\n<div class=\"wp-block-column\" style=\"padding-top:var(--wp--preset--spacing--50);padding-right:0;padding-bottom:var(--wp--preset--spacing--50);padding-left:0\"><!-- wp:group {\"layout\":{\"type\":\"constrained\"}} -->\n<div class=\"wp-block-group\"><!-- wp:heading {\"textAlign\":\"center\",\"style\":{\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|base-2\"}}}},\"textColor\":\"base-2\"} -->\n<h2 class=\"wp-block-heading has-text-align-center has-base-2-color has-text-color has-link-color\">Sed in libero</h2>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"style\":{\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|base-2\"}}}},\"textColor\":\"base-2\"} -->\n<p class=\"has-base-2-color has-text-color has-link-color\">Etiam ut purus mattis mauris sodales aliquam. Sed lectus. Vivamus in erat ut urna cursus vestibulum. Sed cursus turpis vitae tortor.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"style\":{\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|base-2\"}}}},\"textColor\":\"base-2\"} -->\n<p class=\"has-base-2-color has-text-color has-link-color\">Maecenas malesuada. Nam pretium turpis et arcu. Ut non enim eleifend felis pretium feugiat. Praesent vestibulum dapibus nibh.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"style\":{\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|base-2\"}}}},\"textColor\":\"base-2\"} -->\n<p class=\"has-base-2-color has-text-color has-link-color\">Phasellus consectetuer vestibulum elit. Aenean ut eros et nisl sagittis vestibulum. Aliquam erat volutpat. Pellentesque posuere.</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:group --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->",
